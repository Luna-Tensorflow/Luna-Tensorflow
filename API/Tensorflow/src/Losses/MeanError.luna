import Tensorflow.Operations

class MeanErrors:
    MeanErrors

    def meanSquareError yTrue yPred:
        diff = yTrue - yPred
        sq = diff * diff
        m = Operations.mean sq [0]

    def meanAbsoluteError yTrue yPred:
        diff = yTrue - yPred
        ab = Operations.abs diff
        m = Operations.mean ab [0]

