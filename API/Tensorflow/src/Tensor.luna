import Tensorflow.CWrappers.Tensor

# For now we assume tensors are 1D
class Tensor:
    wrapper :: TensorWrapper

    def ptr:
        self.wrapper.ptr

    def length:
        self.wrapper.length

    def atMulti indices:
        self.wrapper.at indices

    def at index:
        self.atMulti [index]

    def toList:
        indices = 0.upto (self.length - 1)
        indices.map self.at

    def toJSON:
        self.toList.toJSON

def tensorFromList list:
    wrapper = create1dTensorWrapper list
    Tensor wrapper
